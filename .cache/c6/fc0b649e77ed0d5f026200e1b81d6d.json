f (currentPage < numPages) {\r\n      return ` \r\n      ${this._markupPreviousBtn(currentPage)}\r\n      ${this._markupNextBtn(currentPage)}\r\n        `;\r\n    }\r\n    return \"\";\r\n  }\r\n  //page 1 and there are not other pages\r\n}\r\nexport default new PaginationView();\r\n"},"lineCount":null}},"error":null,"hash":"194527ab992d5d11d5e9d9a5eb7ea487","cacheData":{"env":{}}}